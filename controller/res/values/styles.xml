<?xml version="1.0" encoding="utf-8"?>
<resources>

	<color name="blue_dark">#001f4d</color>
    <color name="blue_light">#496082</color>
    <color name="bluish_grayish">#eeeff5</color>
    <color name="dark_gray">#515254</color>
    <color name="med_gray">#c8c9ce</color>
    <color name="light_gray">#bdbec2</color>

    <style name="embiggen_theme" parent="android:Theme.Light">
        <!--  theme customization references here -->
        <!-- http://developer.android.com/reference/android/R.styleable.html -->
        <!-- http://developer.android.com/reference/android/R.style.html -->
        <item name="android:actionBarTabStyle">@style/actionBarTabView</item>
    </style>

    <style name="actionBarTabView" parent="@android:style/Widget.Holo.ActionBar.TabView">
        <!-- <item name="android:background">@color/dark_gray</item>-->
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
    </style>

    <style name="mainFontExtraLarge" parent="@android:style/TextAppearance.Large">
        <item name="android:textColor">@color/dark_gray</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">48sp</item>
    </style>

    <style name="mainFontLarge" parent="@android:style/TextAppearance.Large">
        <item name="android:textColor">@color/dark_gray</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="mainFontMedium" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">@color/dark_gray</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="mainFontSmall" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">@color/dark_gray</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="altFontLarge" parent="@android:style/TextAppearance.Large">
        <item name="android:textColor">@color/med_gray</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="altFontMedium" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">@color/med_gray</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="altFontSmall" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">@color/med_gray</item>
        <item name="android:textStyle">normal</item>
    </style>
    

	
</resources>